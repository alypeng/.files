function solarized_light
    set -g fish_color_autosuggestion    brcyan
    set -g fish_color_command           brgreen --bold
    set -g fish_color_comment           brcyan
    set -g fish_color_end               blue
    set -g fish_color_error             red
    set -g fish_color_escape            cyan
    set -g fish_color_match             --background=white
    set -g fish_color_operator          cyan
    set -g fish_color_param             bryellow
    set -g fish_color_quote             yellow
    set -g fish_color_redirection       brmagenta
    set -g fish_color_search_match      --background=white
    set -g fish_color_selection         --background=white
    set -g fish_pager_color_completion  green
    set -g fish_pager_color_description yellow
    set -g fish_pager_color_prefix      cyan --underline
    set -g fish_pager_color_progress
end
